			"type":"sequence",
			"action":[
				{
					"type":"charaMsg",
					"text":["あん？俺にか・・・？\nアリガトよ"],
					"charaID":"5",
					"imgID":"1"
				},
				{
					"type":"systemMsg",
					"text":["大善は喜んでくれたようだ"]
				},
				{
					"type":"fadeout"
